 fig1 = {Highlight.2015_CellStemCell_hiler.fig1.jpg},
 file = {C\:\\Users\\bxu2\\Zotero\\storage\\HKDJGVRG\\mmc1.pdf;C\:\\Users\\bxu2\\Zotero\\storage\\JDCGFUXS\\Hiler 等. - 2015 - Quantification of Retinogenesis in 3D Cultures Rev.pdf;C\:\\Users\\bxu2\\Zotero\\storage\\RSNPN5TI\\S1934590915002593.html},
 issn = {1934-5909},
 journal = {Cell Stem Cell},
 month = {July},
 number = {1},
 pages = {101--115},
 pmcid = {PMC4547539},
 pmid = {26140606},
 title = {Quantification of {{Retinogenesis}} in {{3D Cultures Reveals Epigenetic Memory}} and {{Higher Efficiency}} in {{iPSCs Derived}} from {{Rod Photoreceptors}}},
