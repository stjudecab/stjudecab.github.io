 file = {C\:\\Users\\bxu2\\Zotero\\storage\\J9UCFCGW\\Wei et al. - 2019 - Targeting REGNASE-1 programs long-lived effector T.pdf;C\:\\Users\\bxu2\\Zotero\\storage\\APMGZFP5\\s41586-019-1821-z.html},
 geoid = {GSE126072,Mouse.ATAC.scRNA.RNA.Microarray},
