 issn = {0882-8245},
 journal = {Viral Immunology},
 month = {January},
 pmcid = {PMC4782031},
 pmid = {26741514},
 title = {Hotspots for {{Vitamin}}\textendash{{Steroid}}\textendash{{Thyroid Hormone Response Elements Within Switch Regions}} of {{Immunoglobulin Heavy Chain Loci Predict}} a {{Direct Influence}} of {{Vitamins}} and {{Hormones}} on {{B Cell Class Switch Recombination}}},
