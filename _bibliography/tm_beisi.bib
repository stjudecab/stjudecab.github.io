 doi = {10.1242/dev.124800},
 fig1 = {Highlight.2015_Development_aldiri.fig1.jpg},
 file = {C\:\\Users\\bxu2\\Zotero\\storage\\GCZZIAPV\\Aldiri et al. - 2015 - Brg1 coordinates multiple processes during retinog.pdf;C\:\\Users\\bxu2\\Zotero\\storage\\G2D2IDE3\\4092.html},
