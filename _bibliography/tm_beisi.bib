 file = {C\:\\Users\\bxu2\\Zotero\\storage\\FB8W2AHI\\Science Advances - 2023 - Wang - ZBTB18 restricts chromatin accessibility and prevents transcriptional.pdf},
 geoid = {GSE195944,Mouse.RNA.ATAC},
