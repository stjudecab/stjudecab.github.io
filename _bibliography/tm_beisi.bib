 issn = {1534-5807},
 journal = {Developmental Cell},
 keywords = {CNN RNA-seq,ERCC normalization,Hippo signaling,Myc,NanoString,neural stem cells,neurosphere,radial glia,TEAD,transcriptional amplification},
 month = {December},
 number = {5},
 pages = {576-591.e8},
