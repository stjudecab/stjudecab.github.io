 number = {1},
 pages = {eabq3951},
 pmcid = {PMC9821869},
 pmid = {36608120},
 title = {{{ZBTB18}} Restricts Chromatin Accessibility and Prevents Transcriptional Adaptations That Drive Metastasis},
 volume = {9},
 year = {2023}
