 issn = {2767-9764},
 journal = {Cancer Research Communications},
 keywords = {Cell Line Tumor,DNA Methylation,Gene Expression Regulation Neoplastic,Humans,N-Myc Proto-Oncogene Protein,Neuroblastoma,Polycomb-Group Proteins,Promoter Regions Genetic,Telomerase,Telomere},
